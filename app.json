{
  "expo": {
    "name": "LifeStyleFit",
    "slug": "lifestylefit",
    "description": "App para treinos de Corrida, Caminhada e Ciclismo",
    "githubUrl": "https://github.com/NEVI0/LifeStyle-Fit-App",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.devinc.lifestylefit"
    }
  }
}
